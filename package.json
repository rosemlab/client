{
  "name": "rosem",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "build:ci": "yarn build --report",
    "lint:eslint": "vue-cli-service lint",
    "lint:stylelint": "stylelint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:stylelint": "yarn lint:stylelint \"packages/**/*.{vue,scss}\"",
    "lint:all:markdownlint": "yarn lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint": "run-s lint:all:*",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change",
    "test:unit:ci": "yarn test:unit --coverage --ci",
    "test": "yarn test:unit",
    "test:ci": "yarn test:unit:ci",
    "new": "hygen new",
    "docs": "vuepress dev"
  },
  "gitHooks": {
    "pre-commit_": "cross-env PRE_COMMIT=true lint-staged"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.17",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/vue-fontawesome": "0.1.6",
    "axios": "0.18.0",
    "lodash-es": "^4.17.11",
    "nanoid": "^2.0.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "popper.js": "^1.14.7",
    "vue": "2.6.10",
    "vue-apollo": "^3.0.0-beta.28",
    "vue-class-component": "^7.0.2",
    "vue-meta": "1.6.0",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "3.0.2",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@types/jest": "^24.0.11",
    "@types/lodash-es": "^4.17.3",
    "@vue/cli-plugin-babel": "^3.5.3",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-pwa": "^3.5.1",
    "@vue/cli-plugin-typescript": "^3.5.3",
    "@vue/cli-plugin-unit-jest": "^3.5.3",
    "@vue/cli-service": "^3.5.3",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "express": "^4.16.4",
    "graphql-tag": "^2.10.1",
    "hygen": "^4.0.2",
    "imagemin-lint-staged": "^0.4.0",
    "lerna": "^3.13.1",
    "lint-staged": "^8.1.5",
    "lodash": "^4.17.11",
    "markdownlint-cli": "^0.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-recess-order": "^2.0.1",
    "stylelint-config-standard": "^18.2.0",
    "ts-jest": "^24.0.1",
    "typescript": "^3.4.1",
    "vue-cli-plugin-apollo": "^0.19.2",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^0.14.10"
  },
  "workspaces": {
    "packages": [
      "packages/@rosem/*",
      "packages/@rosem-util/*"
    ]
  },
  "engines": {
    "yarn": ">=1.0.0"
  }
}
