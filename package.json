{
  "name": "rosem",
  "private": true,
  "scripts": {
    "devold": "node_modules/.bin/webpack --mode=development --watch",
    "node": "node -r tsconfig-paths/register",
    "webpack": "yarn node node_modules/webpack/bin/webpack.js",
    "dev": "cross-env NODE_ENV=development yarn webpack --watch",
    "build": "cross-env NODE_ENV=production yarn webpack",
    "build:ci": "yarn build --report",
    "serve": "cross-env NODE_ENV=development yarn node node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "new": "hygen new",
    "test:unit": "jest",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change",
    "test:unit:ci": "yarn test:unit --coverage --ci",
    "test": "yarn test:unit",
    "test:ci": "yarn test:unit:ci"
  },
  "gitHooks": {
    "pre-commit_": "cross-env PRE_COMMIT=true lint-staged"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/types": "^7.7.2",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.3",
    "@types/friendly-errors-webpack-plugin": "^0.1.2",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.22",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.12.7",
    "@types/webpack": "^4.39.8",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "core-js": "^3.4.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "express": "^4.17.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "hygen": "^5.0.1",
    "imagemin-lint-staged": "^0.4.0",
    "lerna": "^3.18.4",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "workspaces": {
    "packages": [
      "packages/@rosemlabs/*"
    ]
  },
  "engines": {
    "node": ">=11.0.0",
    "yarn": ">=1.0.0"
  }
}
