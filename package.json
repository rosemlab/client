{
  "name": "client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "dev:e2e": "vue-cli-service test:e2e --mode=development",
    "build": "vue-cli-service build --modern",
    "build:ci": "yarn build --report",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:stylelint": "yarn lint:stylelint \"packages/**/*.{vue,scss}\"",
    "lint:all:markdownlint": "yarn lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint": "run-s lint:all:*",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change",
    "test:unit:ci": "yarn test:unit --coverage --ci",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test": "run-s test:unit test:e2e",
    "test:ci": "run-s test:unit:ci test:e2e",
    "new": "hygen new",
    "docs": "vuepress dev"
  },
  "gitHooks": {
    "pre-commit_": "cross-env PRE_COMMIT=true lint-staged"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.15",
    "@fortawesome/free-solid-svg-icons": "5.7.2",
    "@fortawesome/vue-fontawesome": "0.1.5",
    "@rosem/app": "0.0.x",
    "axios": "0.18.0",
    "lodash-es": "4.17.11",
    "nanoid": "^2.0.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "popper.js": "^1.14.7",
    "vue": "2.6.7",
    "vue-apollo": "^3.0.0-beta.28",
    "vue-class-component": "^7.0.1",
    "vue-meta": "1.5.8",
    "vue-property-decorator": "^7.3.0",
    "vue-router": "3.0.2",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.x",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@types/jest": "24.0.x",
    "@types/lodash-es": "^4.17.2",
    "@vue/cli-plugin-babel": "3.4.x",
    "@vue/cli-plugin-e2e-cypress": "3.4.x",
    "@vue/cli-plugin-eslint": "3.4.x",
    "@vue/cli-plugin-pwa": "3.4.x",
    "@vue/cli-plugin-typescript": "3.4.x",
    "@vue/cli-plugin-unit-jest": "3.4.x",
    "@vue/cli-service": "3.4.x",
    "@vue/eslint-config-prettier": "4.0.x",
    "@vue/eslint-config-typescript": "4.0.x",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "10.0.x",
    "cross-env": "5.2.x",
    "eslint": "5.14.x",
    "eslint-plugin-vue": "5.2.x",
    "express": "4.16.x",
    "graphql-tag": "2.10.x",
    "hygen": "2.1.x",
    "imagemin-lint-staged": "0.4.x",
    "lerna": "3.13.x",
    "lint-staged": "8.1.x",
    "markdownlint-cli": "0.13.x",
    "npm-run-all": "4.1.x",
    "postcss-preset-env": "6.5.x",
    "stylelint": "9.10.x",
    "stylelint-config-css-modules": "1.3.x",
    "stylelint-config-prettier": "4.0.x",
    "stylelint-config-recess-order": "2.0.x",
    "stylelint-config-standard": "18.2.x",
    "ts-jest": "24.0.x",
    "typescript": "3.3.x",
    "vue-cli-plugin-apollo": "0.19.x",
    "vue-template-compiler": "2.6.7",
    "vuepress": "0.14.x"
  },
  "workspaces": {
    "packages": [
      "packages/@rosem/*",
      "packages/@rosem-util/*"
    ]
  },
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.0.0"
  }
}
