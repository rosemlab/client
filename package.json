{
  "name": "rosem",
  "private": true,
  "scripts": {
    "devold": "node_modules/.bin/webpack --mode=development --watch",
    "node": "node -r tsconfig-paths/register",
    "webpack": "yarn node node_modules/webpack/bin/webpack.js",
    "dev": "cross-env NODE_ENV=development yarn webpack --mode=development --watch",
    "build": "cross-env NODE_ENV=production yarn webpack --mode=production",
    "build:ci": "yarn build --report",
    "new": "hygen new"
  },
  "gitHooks": {
    "pre-commit_": "cross-env PRE_COMMIT=true lint-staged"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/generator": "^7.6.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/template": "^7.6.0",
    "@babel/types": "^7.6.3",
    "@types/jest": "^24.0.20",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.11.7",
    "@types/webpack": "^4.39.8",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-webpack-alias-7": "^0.1.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "express": "^4.17.1",
    "graphql-tag": "^2.10.1",
    "html-loader": "^0.5.5",
    "hygen": "^5.0.1",
    "imagemin-lint-staged": "^0.4.0",
    "json-loader": "^0.5.7",
    "lerna": "^3.18.3",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "workspaces": {
    "packages": [
      "packages/@rosemlabs/*"
    ]
  },
  "engines": {
    "node": ">=11.0.0",
    "yarn": ">=1.0.0"
  }
}
