{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/@rosem/*"
  ],
  "scripts": {
    "bootstrap": "./node_modules/.bin/lerna bootstrap",
    "packages": "./node_modules/.bin/lerna ls",
    "compile": "lerna run compile",
    "build": "vue-cli-service build",
    "test": "yarn unit && yarn e2e",
    "e2e": "vue-cli-service test:e2e --headless",
    "lint": "eslint --ext .js,.vue --fix . && stylelint --fix \"src/**/*.vue\" \"src/**/*.scss\" && markdownlint docs/*.md *.md && prettier --list-different --write \"**/*.{js,json,css,scss,vue,md}\"",
    "dev": "vue-cli-service serve",
    "unit": "vue-cli-service test:unit \\.unit\\.js$",
    "dev:e2e": "vue-cli-service test:e2e --mode=development",
    "build:ci": "cross-env ANALYZE=true yarn build",
    "docs": "vuepress dev",
    "test:ci": "yarn lint && yarn unit --coverage --ci && yarn e2e",
    "new": "hygen new",
    "unit:watch": "yarn unit --watch --notify --notifyMode change"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.1",
    "@fortawesome/free-solid-svg-icons": "5.1.1",
    "@fortawesome/vue-fontawesome": "0.1.1",
    "@rosem/vue-app": "0.0.x",
    "axios": "0.18.0",
    "lodash": "4.17.10",
    "nanoid": "^1.1.0",
    "normalize.css": "8.0.0",
    "nprogress": "0.2.0",
    "popper.js": "^1.14.3",
    "vue": "2.5.16",
    "vue-apollo": "^3.0.0-beta.19",
    "vue-meta": "1.5.2",
    "vue-router": "3.0.1",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-export-default-from": "7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.54",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.54",
    "@vue/cli-plugin-babel": "^3.0.0-rc.5",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-rc.5",
    "@vue/cli-plugin-eslint": "3.0.0-rc.5",
    "@vue/cli-plugin-unit-jest": "3.0.0-rc.5",
    "@vue/cli-service": "3.0.0-rc.5",
    "@vue/eslint-config-prettier": "3.0.0-rc.5",
    "@vue/eslint-config-standard": "3.0.0-rc.5",
    "@vue/test-utils": "1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.4.x",
    "babel-loader": "8.0.0-beta.4",
    "cross-env": "^5.2.0",
    "express": "4.16.x",
    "graphql-tag": "^2.9.2",
    "hygen": "^1.6.2",
    "imagemin-lint-staged": "0.3.x",
    "lerna": "3.0.0-beta.21",
    "lint-staged": "7.2.x",
    "markdownlint-cli": "0.11.x",
    "node-sass": "4.9.x",
    "postcss-preset-env": "^5.2.2",
    "sass-loader": "7.0.x",
    "stylelint": "9.3.x",
    "stylelint-config-css-modules": "1.3.x",
    "stylelint-config-prettier": "3.3.x",
    "stylelint-config-recess-order": "2.0.x",
    "stylelint-config-standard": "18.2.x",
    "stylelint-scss": "^3.1.3",
    "vue-cli-plugin-apollo": "^0.15.0",
    "vue-jest": "2.6.x",
    "vue-template-compiler": "2.5.16",
    "vuepress": "^0.12.0",
    "webpack-bundle-analyzer": "2.13.x"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit_": "cross-env PRE_COMMIT=true lint-staged"
  },
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.0.0"
  }
}
