{
  "name": "@rosemlabs/vue-app",
  "version": "0.0.1",
  "description": "Vue application",
  "keywords": [
    "rosem",
    "rosemlabs",
    "vue-app"
  ],
  "author": "Roman Shevchenko <iroman.via@gmail.com>",
  "homepage": "",
  "license": "MIT",
  "main": "index.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "docs": "vuepress dev",
    "lint:eslint": "vue-cli-service lint",
    "lint:stylelint": "stylelint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:stylelint": "yarn lint:stylelint \"packages/**/*.{vue,scss}\"",
    "lint:all:markdownlint": "yarn lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint": "run-s lint:all:*",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change",
    "test:unit:ci": "yarn test:unit --coverage --ci",
    "test": "yarn test:unit",
    "test:ci": "yarn test:unit:ci",
    "tsc": "tsc"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.26",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "@fortawesome/vue-fontawesome": "0.1.8",
    "axios": "0.19.0",
    "lodash": "^4.17.15",
    "nanoid": "^2.1.7",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "popper.js": "^1.16.0",
    "register-service-worker": "^1.6.2",
    "vue": "2.6.11",
    "vue-apollo": "^3.0.2",
    "vue-class-component": "^7.2.2",
    "vue-meta": "2.3.2",
    "vue-router": "3.1.5",
    "vuex": "3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-pwa": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "^1.0.0-beta.30",
    "eslint-plugin-vue": "^6.1.1",
    "markdownlint-cli": "^0.20.0",
    "postcss-preset-env": "^6.7.0",
    "stylelint": "^12.0.1",
    "stylelint-config-css-modules": "^2.1.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-standard": "^19.0.0",
    "vue-cli-plugin-apollo": "^0.21.3",
    "vue-property-decorator": "^8.3.0",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.2.0"
  }
}
