{
  "name": "rosemlabs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "webpack": "node -r tsconfig-paths/register node_modules/webpack/bin/webpack.js",
    "dev": "yarn webpack --mode=development --watch",
    "devold": "node_modules/.bin/webpack --mode=development --watch",
    "devvue": "vue-cli-service serve",
    "build": "yarn webpack --mode=production",
    "build:ci": "yarn build --report",
    "lint:eslint": "vue-cli-service lint",
    "lint:stylelint": "stylelint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:stylelint": "yarn lint:stylelint \"packages/**/*.{vue,scss}\"",
    "lint:all:markdownlint": "yarn lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint": "run-s lint:all:*",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change",
    "test:unit:ci": "yarn test:unit --coverage --ci",
    "test": "yarn test:unit",
    "test:ci": "yarn test:unit:ci",
    "new": "hygen new",
    "docs": "vuepress dev"
  },
  "gitHooks": {
    "pre-commit_": "cross-env PRE_COMMIT=true lint-staged"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/vue-fontawesome": "0.1.7",
    "axios": "0.19.0",
    "lodash": "^4.17.15",
    "nanoid": "^2.1.3",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "popper.js": "^1.15.0",
    "register-service-worker": "^1.6.2",
    "vue": "2.6.10",
    "vue-apollo": "^3.0.0-rc.7",
    "vue-class-component": "^7.1.0",
    "vue-meta": "2.3.1",
    "vue-router": "3.1.3",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/generator": "^7.6.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/template": "^7.6.0",
    "@babel/types": "^7.6.3",
    "@types/jest": "^24.0.18",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.7.12",
    "@types/webpack": "^4.39.3",
    "@typescript-eslint/eslint-plugin": "^2.5.1-alpha.2",
    "@vue/cli-plugin-babel": "^4.0.0-rc.8",
    "@vue/cli-plugin-eslint": "^4.0.0-rc.8",
    "@vue/cli-plugin-pwa": "^4.0.0-rc.8",
    "@vue/cli-plugin-typescript": "^4.0.0-rc.8",
    "@vue/cli-plugin-unit-jest": "^4.0.0-rc.8",
    "@vue/cli-service": "^4.0.0-rc.8",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-webpack-alias-7": "^0.1.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "express": "^4.17.1",
    "graphql-tag": "^2.10.1",
    "html-loader": "^0.5.5",
    "hygen": "^4.0.11",
    "imagemin-lint-staged": "^0.4.0",
    "json-loader": "^0.5.7",
    "lerna": "^3.17.0",
    "lint-staged": "^9.4.2",
    "markdownlint-cli": "^0.18.0",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-css-modules": "^1.5.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-standard": "^19.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.0-dev.20191021",
    "vue-cli-plugin-apollo": "^0.21.1",
    "vue-property-decorator": "^8.2.2",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.2.0",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9"
  },
  "workspaces": {
    "packages": [
      "packages/@rosemlabs/*"
    ]
  },
  "engines": {
    "node": ">=9.6.0",
    "yarn": ">=1.0.0"
  }
}
